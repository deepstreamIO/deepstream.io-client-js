{
  "name": "deepstream.io-client-js",
  "version": "2.3.0",
  "description": "the javascript client for deepstream.io",
  "main": "dist/lib/client.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "tsc": "rm -rf dist && tsc",
    "build": "webpack",
    "rebuild": "rimraf dist && webpack",
    "clean": "rimraf dist",
    "lint": "tslint test/**/*.ts src/**/*.ts -t verbose",
    "lint:fix": "tslint --fix test/**/*.t src/**/*.ts -t verbose",
    "test": "npm run tsc && mocha dist/test --recursive --reporter dot",
    "test:coverage": "npm run tsc && istanbul cover _mocha dist/test -- --recursive --reporter dot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepstreamIO/deepstream.io-client-js.git"
  },
  "optionalDependencies": {
    "utf-8-validate": "^3.0.3",
    "bufferutil": "^3.0.2"
  },
  "dependencies": {
    "component-emitter2": "^1.3.5",
    "ws": "^3.2.0"
  },
  "browser": {
    "net": false,
    "ws": false
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.46",
    "@types/sinon": "^2.3.7",
    "@types/ws": "^3.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.1",
    "ts-loader": "^3.0.5",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1"
  },
  "author": "deepstreamHub GmbH",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/deepstreamIO/deepstream.io-client-js/issues"
  },
  "homepage": "http://deepstream.io",
  "eslintConfig": {
    "extends": "deepstream"
  }
}
